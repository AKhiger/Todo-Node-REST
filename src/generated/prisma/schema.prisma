generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Todo {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  title       String
  description String?
  isDone      Boolean @default(false)
}
